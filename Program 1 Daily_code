
/*Given a list of numbers and a number k, 
Return whether any two numbers from the list add up to k.
For example, given [10, 15, 3, 7]
k of 17, return true since 10 + 7 is 17 */


#include<iostream>
using namespace std;
int check(int arr[], int n, int i, int k) 
{
int ans, j;

for(i=0;i<n;i++)
{
ans=arr[i];
for(j=1;j<n;j++)
{

if(k==(ans+arr[j]))
{
cout<<"True";


}
}
break;
}

}

int main()
{
int n,i, k;
cout<<"Enter Size of array: ";
cin>>n;
cout<<"Enter the numbers: ";



int arr[n];

for(i=0;i<n;i++)
{
cin>>arr[i];
}
cout<<"Enter k";
cin>>k;

cout<<check(arr,n,i, k);


return 0;
}
